    <author email="hendrik.steidl@rwth-aachen.de">Hendrik Steidl</author>
    <maintainer email="schaefer@embedded.rwth-aachen.de">Simon Sch√§fer</maintainer>
    <license>MIT</license>

    <depend>ssl_vehicle_tracking_msgs</depend>

